--MONDAY HOMEWORK
--Q#1: How many actors are there with the last name ‘Wahlberg’? --
SELECT COUNT (last_name)
FROM actor
WHERE last_name = 'Wahlberg';
--There's 2 actors with the last name Wahlberg--

--Q#2: How many payments were made between $3.99 and $5.99?--

SELECT COUNT (amount)
FROM payments
WHERE amount >= 3.99 AND amount <= 5.99;
--5607 payments--

--Q#3: What film does the store have the most of?--
SELECT COUNT(film_id)
FROM inventory
ORDER BY COUNT(film_id)
--idk--

--Q#4: How many customers have the last name ‘William’?--
SELECT COUNT(last_name)
FROM customer
WHERE last_name = 'William';
--No customer with the last name William--

--Q#5: What store employee (get the id) sold the most rentals?--
SELECT COUNT(staff_id)
FROM rental
GROUP BY staff_id
ORDER BY COUNT(staff_id)
---IDKKKKK---

